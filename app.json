{
  "expo": {
    "name": "Lectoplex",
    "slug": "lectoplex",
    "version": "1.0.0",
    "sdkVersion": "48.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "bundleIdentifier": "com.diomedse.lectoplex"
    },
    "android": {
      "package": "com.diomedse.lectoplex"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "A React Native project using Expo."
  }
}